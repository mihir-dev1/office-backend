{
  "name": "office-backend-starter",
  "version": "1.0.0",
  "description": "This starter contains 3 services: auth-service, company-service, employee-service. Run with Docker Compose locally (creates MongoDB and RabbitMQ containers).",
  "main": "index.js",
  "scripts": {
    "start:auth": "cd services/auth-service && npm run dev",
    "start:company": "cd services/company-service && npm run dev",
    "start:employee": "cd services/employee-service && npm run dev",
    "start:all": "concurrently \"npm run start:auth\" \"npm run start:company\" \"npm run start:employee\""
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "amqplib": "^0.10.8",
    "bcrypt": "^6.0.0",
    "cors": "^2.8.5",
    "dotenv": "^17.2.1",
    "express": "^5.1.0",
    "express-rate-limit": "^8.0.1",
    "helmet": "^8.1.0",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.18.0",
    "zod": "^4.1.0"
  },
  "devDependencies": {
    "@types/bcrypt": "^6.0.0",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^24.3.0",
    "concurrently": "^9.2.0",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.9.2"
  }
}